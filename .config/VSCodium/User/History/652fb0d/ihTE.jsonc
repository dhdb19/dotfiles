{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "pipe": true,
    "separator": " │",
    "constants": [
      "\u001b[28C"
    ],
    "color": {
      "keys": "green",
      "title": "green"
    },
    "key": {
      "width": 12,
      // "type": "string"
    }
  },
  "logo": {
    "type": "auto",
    "source": "~/.config/fastfetch/ibm_fat",
    "color": {
      "1": "green",
      "2": "green"
    }
  },
  "modules": [
