    {
      "type": "shell",
      "key": "│ >SHELL │{$1}"
    },
    {
      "type": "custom",
      "format": "├────────┼─────────────────────────────┤"
    },
    {
      "type": "display",
      "key": "│ >DSPLY │{$1}",
      "format": "{width}x{height}@{refresh-rate}"
