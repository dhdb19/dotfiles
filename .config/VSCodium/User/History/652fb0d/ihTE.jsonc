    },
    {
      "type": "custom",
      "format": "│                                      │"
    },
    {
      "type": "custom",
      "format": "│           I B M   U N I X            │"
    },
    {
      "type": "custom",
      "format": "│                                      │"
    },
    {
      "type": "custom",
      "format": "├────────┬─────────────────────────────┤"
    },
    {
      "type": "os",
      "key": "│ >OPSYS │{$1}",
      "format": "{name}"
    },
